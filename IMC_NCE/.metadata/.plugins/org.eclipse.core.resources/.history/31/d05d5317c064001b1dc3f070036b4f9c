package br.senai.sp.jandira;

import br.senai.sp.jandira.model.Pessoa;

public class Academia {

	public static void main(String[] args) {
		Pessoa pessoa1 = new Pessoa();
		pessoa1.setSexo("M");
		System.out.println(pessoa1.getSexo()); 
		pessoa1.setPeso(40);
		pessoa1.setAltura(1.70);
		System.out.printf("Seu IMC é: %.2f", pessoa1.imc());
		pessoa1.getStatus();
		
	}

}
